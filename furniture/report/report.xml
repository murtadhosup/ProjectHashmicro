<odoo>

    <!-- Report Supplier -->
    <record id="report_supplier_xlsx" model="ir.actions.report">
        <field name="name">Supplier Excel</field>
        <field name="model">furniture.supplier</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">furniture.report_supplier_xlsx</field>
        <field name="report_file">furniture.report_supplier_xlsx</field>
        <field name="binding_model_id" ref="model_furniture_supplier"/>
        <field name="binding_type">report</field>
    </record>

    <!-- Report Penjualan excel -->
    <record id="report_penjualan_xlsx" model="ir.actions.report">
        <field name="name">Penjualan Excel</field>
        <field name="model">furniture.penjualan</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">furniture.report_penjualan_xlsx</field>
        <field name="report_file">furniture.report_penjualan_xlsx</field>
        <field name="binding_model_id" ref="model_furniture_penjualan"/>
        <field name="binding_type">report</field>
    </record>

 <!-- Report penjualan pdf-->
    <record id="report_faktur_pdf" model="ir.actions.report">
        <field name="name">Print Faktur</field>
        <field name="model">furniture.penjualan</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">furniture.report_faktur_pdf</field>
        <field name="report_file">furniture.report_faktur_pdf</field>
        <field name="binding_model_id" ref="model_furniture_penjualan"/>
        <field name="binding_type">report</field>
    </record>
</odoo>